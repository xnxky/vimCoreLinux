<C-H>	basic.vim	/^noremap <C-H> <C-W>h$/;"	m
<C-J>	basic.vim	/^noremap <C-J> <C-W>j$/;"	m
<C-K>	basic.vim	/^noremap <C-K> <C-W>k$/;"	m
<C-L>	basic.vim	/^noremap <C-L> <C-W>l$/;"	m
<C-n>	quickfix.vim	/^noremap <silent> <C-n> :cn <cr>$/;"	m
<C-p>	quickfix.vim	/^noremap <silent> <C-p> :cp <cr>$/;"	m
<F2>	paste.vim	/^nnoremap <F2> :set invpaste paste?<CR>$/;"	m
<c-f11>	eclim.vim	/^nnoremap <silent> <c-f11> :call eclim#vimplugin#FeedKeys('Ctrl+F11', 1)<cr>$/;"	m
<c-i>	buffer.vim	/^nnoremap <silent> <c-i> :BufSurfBack<CR>$/;"	m
<c-o>	buffer.vim	/^nnoremap <silent> <c-o> :BufSurfForward<CR>$/;"	m
<c-space>	autocomp.vim	/^inoremap <silent> <c-space> <c-x><c-o>$/;"	m
<esc>	basic.vim	/^nnoremap <esc> :noh<CR><esc>$/;"	m
<f11>	eclim.vim	/^nnoremap <silent> <f11> :call eclim#vimplugin#FeedKeys('F11', 1)<cr>$/;"	m
<leader>ca	quickfix.vim	/^noremap <leader>ca  :execute "caddexpr '".GetCommand()."Message'"$/;"	m
<leader>f	nerdtree.vim	/^noremap <silent> <leader>f :NERDTreeFind<CR>$/;"	m
<leader>gl	ctags.vim	/^nnoremap <leader>gl :!(ctags --file-scope=no *)&$/;"	m
<leader>gr	ctags.vim	/^nnoremap <leader>gr :!(ctags --file-scope=no -R .)&$/;"	m
